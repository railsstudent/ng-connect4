{
  "name": "ng-connect4",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sr-tests": "ng test --watch=false",
    "build-prod": "ng build --prod --build-optimizer false",
    "start:prod": "http-server dist/ng-connect4 -p 8000",
    "code-coverage": "ng test --watch=false --code-coverage",
    "start:coverage": "http-server coverage/ -p 8001"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "pre-push": "npm run sr-tests"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "trailingComma": "none"
  },
  "lint-staged": {
    "linters": {
      "{src,test,e2e}/**/*.ts": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@fortawesome/angular-fontawesome": "0.1.1",
    "@fortawesome/fontawesome-svg-core": "1.2.2",
    "@fortawesome/free-solid-svg-icons": "5.2.0",
    "@ngrx/store": "6.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "core-js": "2.5.4",
    "es6-object-assign": "1.1.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular/cli": "~8.3.22",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@ngrx/schematics": "^6.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "http-server": "0.11.1",
    "husky": "^1.0.0-rc.13",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "7.1.2",
    "prettier": "^1.13.7",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.5.3"
  }
}