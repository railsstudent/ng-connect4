{
  "name": "ng-connect4",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sr-tests": "ng test --watch=false",
    "build-prod": "ng build --prod --build-optimizer false",
    "start:prod": "npx serve dist/ng-connect4 -l 8000",
    "code-coverage": "ng test --watch=false --code-coverage",
    "start:coverage": "npx serve coverage/ -p 8001"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "pre-push": "npm run sr-tests"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "{src,test,e2e}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.0",
    "@angular/common": "12.0.0",
    "@angular/compiler": "12.0.0",
    "@angular/core": "12.0.0",
    "@angular/forms": "12.0.0",
    "@angular/platform-browser": "12.0.0",
    "@angular/platform-browser-dynamic": "12.0.0",
    "@angular/router": "12.0.0",
    "@fortawesome/angular-fontawesome": "0.1.1",
    "@fortawesome/fontawesome-svg-core": "1.2.2",
    "@fortawesome/free-solid-svg-icons": "5.2.0",
    "@ngrx/store": "12.0.0",
    "@ngrx/store-devtools": "12.0.0",
    "core-js": "2.5.4",
    "es6-object-assign": "1.1.0",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@ngrx/schematics": "^12.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "9.5.0",
    "prettier": "^1.19.1",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
